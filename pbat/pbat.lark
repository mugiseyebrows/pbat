start: (name "=")? name "(" args? ")"

args: arg ("," arg)*

arg: ARG ("=" ARG)?
   | list

list: "[" ARG ("," ARG)* "]" | "[" "]"

ARG: (LETTER | NUMBER | PUNCT | WS)+
   | "\"" (LETTER | NUMBER | PUNCT | WS | "(" | ")" | ",")+ "\""

name: /[A-Za-z0-9_]+/

LETTER: ("а".."я") | ("А".."Я") | /[A-Za-z]/

NUMBER: /[0-9]+/

PUNCT: /[\\\/:.@_%~-]/

%import common.WS

%ignore WS
